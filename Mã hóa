local HttpService = game:GetService("HttpService")

-- URL của tệp tin từ GitHub
local url = "https://raw.githubusercontent.com/skyemngu12/Anime-last-stand/refs/heads/main/Hlw%20aa"

-- Lấy nội dung từ GitHub
local response = HttpService:GetAsync(url)

-- Mã hóa nội dung thành Base64
local base64EncodedData = HttpService:UrlEncode(response)

-- In ra kết quả mã hóa
print("Dữ liệu đã mã hóa thành Base64:")
print(base64EncodedData)
