local HttpService = game:GetService("HttpService")

-- Địa chỉ URL của tệp tin GitHub bạn muốn mã hóa
local url = "https://raw.githubusercontent.com/skyemngu12/Anime-last-stand/refs/heads/main/Hlw%20aa"  -- Thay bằng URL của tệp bạn muốn mã hóa

-- Lấy nội dung tệp tin từ GitHub
local fileContent = HttpService:GetAsync(url)

-- Mã hóa nội dung tệp tin thành Base64
local base64EncodedContent = HttpService:UrlEncode(fileContent)  -- Đây là mã hóa URL, bạn có thể dùng Base64 nếu cần, nhưng phương pháp này là cách tiếp cận trong Roblox

-- In ra kết quả Base64 đã mã hóa
print("Nội dung tệp đã mã hóa (Base64):")
print(base64EncodedContent)
