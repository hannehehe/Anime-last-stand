local function hexToString(hex)
    return hex:gsub('%x%x', function(cc)
        return string.char(tonumber(cc, 16))
    end)
end

-- Đây là chuỗi Hex đã mã hóa (thay bằng giá trị bạn thu được)
local encodedScript = ""; 

-- Giải mã Hex và thực thi script Lua
local decodedScript = hexToString(encodedScript)
loadstring(decodedScript)()
